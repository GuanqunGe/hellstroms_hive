<!--Done on 06/28/19 for 1e1p selection using v12 samples-->
<?xml version="1.0" ?>

<!--General Precuts -->
<precut def ="reco_flash_total_pe_in_beamgate[0] > 20" name ="Optical Filter 20 PE"/>
<precut def = "(reco_vertex_x > 10 &amp;&amp; reco_vertex_x < 246 &amp;&amp; reco_vertex_y > -107 &amp;&amp; reco_vertex_y < 107 &amp;&amp; reco_vertex_z > 10 &amp;&amp; reco_vertex_z < 1026) " name = "Fiducial Vertex Cut" />

<!--Shower & Track Precuts -->
<precut def = "reco_shower_energy_max[0]>0.02" name = "Shower Energy Cut" />
<precut def = "(TMath::Finite(reco_shower_dEdx_amalgamated[0]))" name = "Shower dE/dx Plane Min Nhits"/>
<precut def = "(TMath::Finite(reco_track_good_calo_best_plane[0]))" name = "good calo plane track" />
<precut def = "reco_track_displacement[0]<250" name = "Track Length Cut" />


<!--Comsic BDT Training + Spectator Variables-->
<mva type="BDT" use="yes" tag = "COSMIC" name ="Cosmic focused BDT" binning = "(26,0.15,0.75)">
<method type = "COSMIC" use = "yes">
<param>  NTrees=3000				</param>
<param>  MinNodeSize=2.5%			</param>
<param>  BoostType=RealAdaBoost	    </param>
<param>  AdaBoostBeta=0.25			</param>
<param>  SeparationType=GiniIndex   </param>
<param>  UseBaggedBoost				</param>
<param>  BaggedSampleFraction=0.5   </param>
<param>  nCuts=-1					</param>
<param>  MaxDepth=2					</param>
</method>



<!-- Energies -->
<var def = "mctruth_leading_exiting_proton_energy-0.93828" binning = "(48,0,1)" unit = "true proton energy [GeV]" type = "d" spectator = "true"/>
<var def = "0+reco_shower_energy_max[0]/1000.0" binning = "(14,0,1)" unit = "Reconstructed Shower Energy [GeV]" type = "d" spectator = "false" />
<var def = "0+reco_shower_energy_max[0]/1000.0" binning = "(14,0,1)" unit = "Reconstructed Shower Energy Log [GeV]" type = "d" spectator = "true" logplot = "true"/>
<var def = "1/1*reco_track_proton_kinetic_energy[0]" binning = "(28,0,2)" unit = "Reconstructed Track Kinetic Energy [GeV]" type = "d" spectator = "true" />

<var def = "3/3*reco_shower_energy_max[0]/1000.0+reco_track_proton_kinetic_energy[0]" binning = "(48,0,2)" unit = "DEMO Reconstructed Nue Energy [GeV]" type = "d" spectator = "true" />

<var def = "4/4*(0.5*(2*(0.93957-0.04)*(reco_shower_energy_max[0]/1000)-(pow((0.93957-0.04),2)+(-pow(0.93827,2))))/((0.93957-0.04)-(reco_shower_energy_max[0]/1000)+(reco_shower_energy_max[0])/1000*reco_shower_dirz[0]))-reco_shower_energy_max[0]/1000+(reco_track_proton_kinetic_energy[0]+0.938272)-(0.93957-0.04)" binning = "(20,0,2)" unit = "QE_{#nu}^{QE}[e]-E^{range} [GeV]" type = "d" spectator = "false" />

<!-- Momentum -->
<!-- Cosmic Exclusive-->
<var def = "5/5*0.5*(abs(reco_shower_diry[0]+reco_track_diry[0]))" binning = "(32,0,2)" unit = "Portion of Total Momentum Aligned to the y-direction" type = "d" spectator = "false" /> 
<!-- Cosmic Exclusive-->
<var def = "6/6*sqrt(pow((reco_track_proton_kinetic_energy[0]+0.938272)*(reco_track_dirx[0])+(reco_shower_energy_max[0]*0.001)*reco_shower_dirx[0],2)+pow(    (reco_track_proton_kinetic_energy[0]+0.938272)*(reco_track_diry[0])+(reco_shower_energy_max[0]*0.001)*reco_shower_diry[0],2))" binning = "(24,0,2)" unit = "Reconstucted Nue Transverse (YX) Momentum [GeV]" type = "d" spectator = "false"/>



<var def = "7/7*abs((reco_track_dirz[0]*sqrt(pow(reco_track_proton_kinetic_energy[0]+0.938272,2)-pow(0.938272,2))+reco_shower_dirz[0]*reco_shower_energy_max[0]/1000))" binning = "(30,0,2)" unit = "Total Momentum on beam-direction [GeV/c]" type = "d" spectator = "false" />

<var def = "8/8*((reco_track_dirz[0]*sqrt(pow(reco_track_proton_kinetic_energy[0]+0.938272,2)-pow(0.938272,2))+reco_shower_dirz[0]*reco_shower_energy_max[0]/1000)/(sqrt(1-reco_track_dirz[0]*reco_track_dirz[0])*sqrt(pow(reco_track_proton_kinetic_energy[0]+0.938272,2)-pow(0.938272,2))+sqrt(1-reco_shower_dirz[0]*reco_shower_dirz[0])*reco_shower_energy_max[0]/1000))" binning = "(30,0,8)" unit = "Ratio of Momentum on z-direction versus on xy-plane" type = "d" spectator = "false" />
<!-- Stopping Power -->

<var def = "9/9*reco_shower_dEdx_amalgamated[0]" binning = "(24,0,6.5)" unit = "Amalgamated Shower dE/dx (All Planes) [mev/cm]" type="d" spectator="false"/>
<var def = "10/10*(fabs(max(reco_track_mean_trunc_dEdx_start_half_best_plane[0],reco_track_mean_trunc_dEdx_end_half_best_plane[0])-reco_shower_dEdx_plane2_max[0]))" binning = "(30,0,15)" unit =  "Difference of dE/dx btw a Track and a Shower v2 [MeV/cm]" type = "d" spectator = "false" />
<var def = "11/11*abs(reco_track_trunc_PIDA_best_plane[0]-10)" binning = "(28,0,24)" unit = "Track PIDA (respected to 10)" type = "d" spectator = "false" />

<var def = "13/13*abs((max( reco_track_mean_trunc_dEdx_start_half_best_plane[0], reco_track_mean_trunc_dEdx_end_half_best_plane[0])))" binning = "(48,0,24)" unit = "Track Edge Maximum dEdx [MeV/m]" type = "d" spectator = "false" />
<!-- Length -->

<var def = "14/14*reco_shower_length[0]" binning = "(24,0,100)" unit = "Shower Length [cm]" type = "d" spectator="false"/>
<var def = "15/15*reco_track_displacement[0]" binning = "(14,0,300)" unit = "Track Displacement [cm]" type = "d" spectator = "false" />
<var def = "16/16*abs(max(reco_track_displacement[0],reco_shower_length[0])-50)" binning = "(30,0,350)" unit = "Maximum Length of a Track or a Shower (respected to 50) [cm]" type = "d" spectator = "false" />
<var def = "17/17*abs(max(reco_track_displacement[0],reco_shower_length[0]))" binning = "(30,0,350)" unit = "Maximum Length of a Track or a Shower [cm]" type = "d" spectator = "true" />

<!-- Distance btw shower and track-->

<var def = "18/18*min(min(reco_track_endz[0]-reco_shower_startz[0],(reco_track_startz[0]-reco_shower_startz[0])),min(min(-reco_track_endy[0]+reco_shower_starty[0],(-reco_track_starty[0]+reco_shower_starty[0])),min(reco_track_endx[0]-reco_shower_startx[0],(reco_track_startx[0]-reco_shower_startx[0]))))" binning = "(50,-100,5)" unit = "A Track Ahead of a Shower (all-direction)[cm]" type = "d" spectator = "false" />

<!-- Others -->

<!--<var def =  "19/19*reco_shower_nuscore[0]" binning = "(24, 0, 1)" unit = "Reco. Shower Associated Slice Neutrino Score" type = "d" spectator = "false" />-->
<var def = "20/20*reco_shower_opening_angle[0]" binning = "(20,0,0.8)" unit = "Shower Opening Angle [rad]" type = "d" spectator = "false" />
<var def = "21/21*2*(reco_shower_length[0]*tan(reco_shower_opening_angle[0]/2))" binning = "(25,0,20)" unit = "Base Length of a Shower [cm]" type = "d" spectator = "false" />

<var def = "100/100*min(reco_track_endz[0]-reco_shower_startz[0],(reco_track_startz[0]-reco_shower_startz[0]))" binning = "(50,-100,5)" unit =  "DEMO A Track Ahead of a Shower (z-direction)[cm]" type = "d" spectator = "true" />
<var def = "101/101*min(-reco_track_endy[0]+reco_shower_starty[0],(-reco_track_starty[0]+reco_shower_starty[0]))" binning = "(50,-100,5)" unit =  "DEMO A Track Ahead of a Shower (y-direction)[cm]" type = "d" spectator = "true" />
<var def = "102/102*min(reco_track_endx[0]-reco_shower_startx[0],(reco_track_startx[0]-reco_shower_startx[0]))" binning = "(50,-100,5)" unit =  "DEMO A Track Ahead of a Shower (x-direction)[cm]" type = "d" spectator = "true" />
<var def = "103/103*(0.5* (2* (0.93957-0.04)* (reco_shower_energy_max[0]/1000)-(pow((0.93957-0.04),2)+(-pow(0.93827,2))))/((0.93957-0.04)-(reco_shower_energy_max[0]/1000)+reco_shower_energy_max[0]/1000*reco_shower_dirz[0]))" binning = "(40,0,1)" unit =  "DEMO Nue QE [GeV]" type = "d" spectator = "true" />
<!--P for precuts -->
<var def = "104/104*reco_flash_total_pe_in_beamgate[0]" binning = "(10,0,200)" unit =  "DEMOP Number of Photo-Electrons in Beam Gate" type = "d" spectator = "true" />
<var def = "105/105*reco_shower_energy_max[0]/1000" binning = "(40,0,1)" unit =  "DEMOP Reco. Shower Energy" type = "d" spectator = "true" />
<var def = "106/106*reco_track_displacement[0]" binning = "(80,-5,400)" unit =  "DEMOP Reco. Track Length" type = "d" spectator = "true" />
<var def = "107/107* reco_track_good_calo_best_plane[0]" binning = "(120,0,200)" unit =  "DEMOP Calometric hits of reco. Track" type = "d" spectator = "true" />
<var def = "108/108*reco_shower_dEdx_plane2_median[0]" binning = "(30,0,10)" unit =  "DEMOP Shower dE/dx the Median of a 2x4 Region[MeV/cm]" type = "d" spectator = "true" />
<var def = "109/109*reco_shower_dEdx_plane2_nhits[0]" binning = "(28,0,10)" unit = "DEMOP Shower dE/dx Plane Nhits" type = "d" spectator = "true"/>



<!--Shower Geom (Spectator) Variables-->
<var def = "reco_shower_opening_angle[0]" binning = "(24,0,0.8)" unit = "Shower Opening Angle" type = "d" spectator = "true" />
<var def = "cos(reco_shower_theta_yz[0])" binning = "(24,-1,1)" unit = "Reconstructed Shower - Cosine #theta_{yz}" type = "d" spectator = "true"/>
<var def = "cos(reco_shower_phi_yx[0])" binning = "(24,-1,1)" unit = "Reconstructed Shower - Cosine Phi yx" type = "d" spectator = "true"/>
<var def = "reco_shower_startx[0]" binning = "(24,-10,260)" unit = " Reconstructed Shower start X pos [cm]" type = "d" spectator = "true"/>
<var def = "reco_shower_starty[0]" binning = "(24,-120,+120)" unit = " Reconstructed Shower start Y pos [cm]" type = "d" spectator = "true"/>
<var def = "reco_shower_startz[0]" binning = "(24,-10,1060)" unit = " Reconstructed Shower start Z pos [cm]" type = "d" spectator = "true"/>
<var def = "reco_shower_start_to_nearest_dead_wire_plane0[0]" binning = "(24,0,125)" unit = " Reconstructed Shower dead wire dist plane 0 pos [cm]" type = "d" spectator = "true" />
<var def = "reco_shower_start_to_nearest_dead_wire_plane1[0]" binning = "(24,0,110)" unit = " Reconstructed Shower dead wire dist plane 1 pos [cm]" type = "d" spectator = "true" />
<var def = "reco_shower_start_to_nearest_dead_wire_plane2[0]" binning = "(24,0,200)" unit = " Reconstructed Shower dead wire dist plane 2 pos [cm]" type = "d" spectator = "true" />
<var def =  "reco_shower_nuscore[0]" binning = "(24, 0, 1)" unit = "Reconstructed Shower Associated Slice Neutrino Score Log" type = "d" spectator = "true" logplot = "true"/>
<var def =  "reco_shower_nuscore[0]" binning = "(24, 0, 1)" unit = "Reconstructed Shower Associated Slice Neutrino Score" type = "d" spectator = "true" />

<!--Track Geom (Spectator) Variables-->
<var def = "(reco_track_dirx[0]*reco_shower_dirx[0]+reco_track_diry[0]*reco_shower_diry[0]+reco_track_dirz[0]*reco_shower_dirz[0])" binning ="(24,-1,1)" unit="Cosine Angle between photon and proton [rad]" type = "d" spectator = "true"/>
<var def = "cos(reco_track_theta_yz[0])" binning = "(24,-1,1)" unit = "Reconstructed Track - Cosine #theta_{yz}" type = "d" spectator = "true"/>
<var def = "cos(reco_track_phi_yx[0])" binning = "(24,-1,1)" unit = "Reconstructed Track - Cosine Phi yx" type = "d" spectator = "true"/>
<var def = "reco_track_startx[0]" binning = "(24,-10,260)" unit = " Reconstructed Track start X pos [cm]" type = "d" spectator = "true"/>
<var def = "reco_track_starty[0]" binning = "(24,-120,+120)" unit = " Reconstructed Track start Y pos [cm]" type = "d" spectator = "true"/>
<var def = "reco_track_startz[0]" binning = "(24,-10,1060)" unit = " Reconstructed Track start Z pos [cm]" type = "d" spectator = "true"/>
<var def = "reco_track_endx[0]" binning = "(24,-10,260)" unit = " Reconstructed Track end X pos [cm]" type = "d" spectator = "true"/>
<var def = "reco_track_endy[0]" binning = "(24,-120,+120)" unit = " Reconstructed Track end Y pos [cm]" type = "d" spectator = "true"/>
<var def = "reco_track_endz[0]" binning = "(24,-10,1060)" unit = " Reconstructed Track end Z pos [cm]" type = "d" spectator = "true"/>

</mva>

